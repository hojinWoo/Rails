<h1>게시글 수정</h1>

<form action = "/posts/<%= @post.id %>/update/">
  이름 : <input type = "text" name = "username" value="<%= @post.username%>"/>
  <p/>
  제목 : <input type = "text" name = "title" value="<%= @post.title%>"/>
  <p/>
  내용 : <textarea type = "text" name = "content"><%= @post.content%></textarea>
  <input type = "submit" value="등록" />
</form>
