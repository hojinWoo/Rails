<h1>처음화면</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<a href="/posts/new">게시물 작성하기</a>
<% @posts.each do |post|%>
  <p><%= post.id %></p>
  <p><%= post.title %></p>
  <p><%= post.body %></p>
  <p><%= post.created_at %></p>
  <p><%= post.updated_at %></p>
  <a href="/posts/<%=post.id%>">게시글 보기</a>
  <a href="/posts/<%=post.id%>/destroy">게시글 삭제</a>
  <hr />
<% end %>
